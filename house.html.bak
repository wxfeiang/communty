			/*求顶部左侧宽度*/
			var windowWidth = document.body.clientWidth;
			var leftWidth = $(".leftBox").width();
			var ulWidth1 = windowWidth - leftWidth
			$(".commuinityTop ul").width(ulWidth1 - 200);
			$(".commuinityTop ul li").width((ulWidth1 - 200) / 5);
			/*求每个房间的宽度*/
			var ulWidth = $(".houseBox").eq(0).find("ul").width(); //一层的宽度
			$(".houseBox").each(function (i) {
				var li_length = $(".houseBox").eq(i).find("li").length; //每层房间个数
				var li_width = parseInt((ulWidth - li_length * 7) / li_length); //每个房间的宽度取整
				var residueNum = ulWidth - (li_width * li_length + li_length * 5); //总宽减去每个宽度和右边距
				var smallNum = parseInt(residueNum / li_length); //剩余宽度平均分配
				li_width = li_width + smallNum - 2; //减掉边框后的li的宽
				var minusNum = ulWidth - (li_width + 7) * li_length;
				$(".houseBox").eq(i).find("li").each(function (j) {
					var li_width1;
					if (minusNum > 0) {
						li_width1 = li_width + 1;
					} else {
						li_width1 = li_width;
					}
					$(".houseBox").eq(i).find("li").eq(j).width(li_width1);
					minusNum = minusNum - 1;
				})
			})
		})
